@@@editpost@@@
<div style="text-align:left" data-<%=template%>="<%=_tid%>">
    <div style="padding-top:10px;" class="headerheight calcheight">
        <div style="padding:10px 20px 0px 20px">
            <%if(this.groupselect){%>
            <div style="padding-bottom:10px">
                <div style="font-size:16px;"><b>Add To Page</b></div>
                <div class="groupsearchbox" style="position:relative;"></div>
            </div>
            <%}%>
            <div style="font-size:14px;padding:2px;text-align:left;"><b>Post Title</b></div>            
            <div>
                <textarea style="width:100%;height:50px;resize:none;font-size:13px" id="posttitle" class="gloginfield gloginfieldstyle m-corner-all truebox" placeholder="Post Title"><%=title%></textarea>
            </div>
            <%if(this.ts){%>
            <div style="color:#aaa;padding-top:5px">
                Created: <%=ts.formatTime()%>
            </div>
            <%}%>
        </div>
    </div>
    <div data-id="editpostviewscroll" style="position:relative">
        <div class="calcheight">
            <div style="padding:10px 20px">
                <div style="font-size:14px;text-align:left;" id="tagele"><b>Tags</b></div>
                <div style="position:relative">
                    <input type="text" class="m-corner-top truebox searchtags" style="width:140px;padding:5px;border-top:1px solid #ccc;border-left:1px solid #ccc;border-right:1px solid #ccc;" placeholder="Search Tags">
                    <i class="icon-cancel searchcancel menuicon" style="color:black;font-size:18px;position:absolute;top:2px;left:115px;display:none"></i>
               </div>
               <div class="taglist m-corner-tr m-corner-bottom" style="background:#eee;border:1px solid #ccc;padding:2px;"></div>
                <div style="font-size:14px;text-align:left;padding-top:10px"><b>Image</b></div>
                <div id="imagebox"></div>
                <div style="font-size:14px;text-align:left;padding-top:10px"><b>Privacy</b></div>
                <div style="padding-top:5px;">
                    <div class="btn-group">
                        <span class="btn privbtn" data-priv="0"><i class="icon-globe"></i> Public</span>
                        <span class="btn privbtn" data-priv="1"><i class="icon-friends"></i> Community Only</span>
                        <!-- <span class="btn privbtn" data-priv="2"><i class="icon-lock"></i> Only Me</span> -->
                    </div>
                </div>       
            </div>
            <div style="padding:10px 20px 20px 20px;">
                <div style="font-size:14px;padding:2px;text-align:left;"><b>Post Content</b></div>            
                <div style="min-height:100px;margin-top:10px;" id="postcontent" class="redactorbox"><%=content%></div>
            </div>
        </div>
    </div>
    <div style="padding:10px;text-align:center;" class="calcheight footerbar">
        <%if(this.ts){%>
        <span class="btn btn-danger x_delete" style="margin-left:10px"><i class="icon-trash"></i> Delete Post</span>
        <span class="btn btn-primary savebtn" style="margin-left:10px"><i class="icon-floppy"></i> Save Post</span>
        <%}else{%>
        <span class="btn btn-primary savebtn"><i class="icon-floppy"></i> Save Post</span>
        <%}%>
        <div style="position:absolute;bottom:5px;right:5px;color:red;display:none" class="response"></div>
    </div>
</div>
