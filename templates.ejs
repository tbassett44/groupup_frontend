                            <div style="padding:5px 10px;" class="menuicon selecttime" data-time="week">Week</div>
                            <div style="padding:5px 10px;" class="menuicon selecttime" data-time="month">Month</div>
                            <div style="padding:5px 10px;" class="menuicon selecttime" data-time="year">Year</div>
                            <div style="padding:5px 10px;" class="menuicon selecttime" data-time="all">All Time</div>
                        </div>
                    </li>
                </ul>
            </td>
        </tr>
    </table>
    <%}else if(selected.type=='rss'){%>
