@@@community_post_tags@@@
<div data-<%=template%>="<%=_tid%>">
     <%for (var i = 0; i < tags.length; i++) {
    var tag=tags[i];
    if(following.indexOf(tag._id)>=0) tag.following=1;
    else tag.following=0;
    %>
    <div style="padding:10px 5px;border-bottom:1px solid #ccc;" class="tagselect filterbox" data-tagid="<%=tag._id%>">
        <table>
            <tr>
                <td style="width:25px;text-align:center">
                    <span class="ischecked">
                        <i class="icon-check" style="font-size:20px"></i>
                    </span>
                    <span class="notchecked">
                        <i class="icon-check-empty" style="font-size:20px"></i>
                    </span>
                </td>
                <td style="width:25px;text-align:center">
                    <div style="width:20px;height:20px;background:<%=tag.color%>;border:1px solid #ccc;font-size:14px;color:white;" class="m-corner-all"></div>
                </td>
                <td style="font-size:16px">
                    <div>
                        <%=tag.name%>
                    </div>
                    <div style="padding-top:2px;font-size:11px;color:#888;padding-left:5px">
                        <%if(tag.following){%>
                        <span style="color:green"><i class="icon-rss"></i> Following</span>
                        <%}else{%>
                        <span style="color:#888">Not Following</span>
                        <%}%>
                    </div>
                </td>
            </tr>
        </table>
    </div>
<%};%>
</div>
