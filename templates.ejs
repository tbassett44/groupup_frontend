                        <!-- <div class="btn">Daily</div> -->
                        <div class="repeatbtn btn <%if(repeat=='weekly'){%>active<%}%>" data-val="weekly">Weekly</div>
                        <div class="repeatbtn btn <%if(repeat=='monthly'){%>active<%}%>" data-val="monthly">Monthly</div>
                        <div class="repeatbtn btn <%if(repeat=='yearly'){%>active<%}%>" data-val="yearly">Yearly</div>
                    </div>
                </td>
            </tr>
            <tr id="intervalrow" <%if(repeat=='yearly'){%>style="display:none"<%}%>>
