                        <div style="position:absolute;top:25px;left:5px;width:200px;background:#333;color:white;display:none;" id="categorymenu">
                            <%for (var i = 0; i < selected.feeds.length; i++) {
                                var tfeed=selected.feeds[i];
                                %>
                                <div style="padding:5px 10px;" class="menuicon selectfeed <%if(feed._id==tfeed._id){%>activenav<%}%>" data-feed="<%=tfeed._id%>"><%=tfeed.name%></div>
                            <%};%>
                        </div>
                    </li>
                </ul>
            </td>
        </tr>
    </table>

    <div style="position:absolute;top:29px;right:8px;width:200px;background:#333;padding:2px;color:black;display:none;z-index:10" class="m-corner-bottom filtermenu">
        <div style="background:white;">
            <%for (var i = 0; i < selected.feeds.length; i++) {
                var feed=selected.feeds[i];
                %>
                <div style="padding:10px 5px;border-bottom:1px solid #ccc;" class="tagselect filterbox" data-feedid="<%=feed._id%>">
                    <table>
                        <tr>
                            <td style="font-size:16px">
                                <%=feed.name%>
                            </td>
                        </tr>
                    </table>
                </div>
            <%};%>
        </div>
        <div style="height:5px;background:white;" class="m-corner-bottom"></div>
    </div>
    <%}%>
    <div style="position:absolute;top:30px;left:5px;width:200px;background:#333;color:white;z-index:10;display:none" id="newsnavmenu">
        <%for (var i = 0; i < nav.length; i++) {
            var navitem=nav[i];
            %>
            <div style="padding:6px 10px;font-size:16px;color:white" data-id="<%=navitem._id%>" class="newsnavbutton menuicon <%if(navitem._id==selected._id){%>activenav<%}%>"><%=navitem.name%></div>
        <%};%>
    </div>
</div>
