@@@add_promoted@@@
<div data-<%=template%>="<%=_tid%>" style="text-align:left;">
    <div style="padding:15px;">
        <div style="font-size:20px;color:#888;padding-bottom:10px;text-align:left"><b>Promotion Editor</b></div>
        <div style="padding:10px;">
            <div style="font-size:12px">The Promotion Editor allows you to promote groups, businesses, organizations, websites, projects, and more.</div>
            <div style="padding-top:20px;">
                <div style="font-size:13px;padding-bottom:5px;padding-top:10px;"><b>Title</b></div>
                <div>
                    <textarea style="width:100%;min-height:40px;padding:5px;border:1px solid #ccc;resize:none" class="truebox s-corner-all" id="promoted_title" maxlength="150"><%=title%></textarea>
                </div>
                <div style="font-size:13px;padding-bottom:5px;padding-top:20px;"><b>Description</b></div>
                <div>
                    <textarea maxlength="2000" style="width:100%;min-height:40px;padding:5px;border:1px solid #ccc;resize:none" class="truebox s-corner-all" id="promoted_description"><%=description%></textarea>
                </div>
                <div style="font-size:13px;padding-bottom:5px;padding-top:20px;" id="promoimg"><b>Image</b></div>
                <div>
                    <div id="imagebox"></div>
                </div>
                <div style="font-size:13px;padding-bottom:5px;padding-top:10px;"><b>Page Link</b></div>
                <div style="text-align:left;padding-bottom:10px">
                    <span class="gbtn s-corner-left active pagepicker" data-pageid="custom">Custom Link</span>
                    <span class="gbtn s-corner-right pagepicker" data-pageid="groupup" style="margin-left:-1px">GroupUp Page</span>
                </div>
                <div>
                    <div data-page="groupup" class="promopage" style="display:none;">
                        Coming Soon!
                        <!-- <input style="width:250px;padding:5px;border:1px solid #ccc;" id="pagesearch" placeholder="Search GroupUp Pages" class="s-corner-all"> -->
                    </div>
                    <div data-page="custom" class="promopage">
                        <input style="width:250px;padding:5px;border:1px solid #ccc;" id="remotelink" placeholder="Webapge URL" class="s-corner-all" value="<%if(link&&link.url){%><%=link.url%><%}%>">
                    </div>
                </div>
                <div style="font-size:13px;padding-bottom:5px;padding-top:10px;"><b>Promotion Type</b> <span style="font-size:12px;color:#888">(Select All That Apply)</span></div>
                <div id="typelist">
                    <%for (var i = 0; i < types.length; i++) {
                        var type=types[i];%>
                        <div data-id="<%=type._id%>" style="display:inline-block;float:left;padding:3px;margin-left:3px;margin-top:3px;color:black;border:1px solid #ccc;cursor:pointer" class="s-corner-all togglelisttype">
                            <i class="<%if(tags.indexOf(type._id)>=0){%>icon-check<%}else{%>icon-check-empty<%}%>"></i> <%=type.name%>
                        </div>
                    <%};%>
                </div>
            </div>
            <div></div>
        </div>
    </div>
    <div style="padding:20px;text-align:center">
        <span class="btn btn-primary x_save"><i class="icon-floppy"></i> Save</span>
        <%if(this.ts){%>
        <span class="btn btn-danger x_delete" style="margin-left:10px"><i class="icon-trash"></i> Delete</span>
        <%}%>
    </div>
    <div class="x_response noticetext" style="padding:5px;text-align:center;display:none"></div>
</div>
