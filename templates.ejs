                <td>
                    <div style="font-size:16px;padding:4px;text-align:left">
                        <%if(this.canceled){%>[CANCELED] <%}%><%=otitle%>
                    </div>
                    <div style="font-size:13px;color:#aaa;padding-left:5px;padding-bottom:5px;text-align:left">
                        <div>
                            <%=timestring%>
                        </div>
                        <div style="padding-top:4px">
                            <i class="<%if(this.priv&&parseInt(this.priv,10)){%>icon-lock<%}else{%>icon-globe<%}%>"></i>&nbsp;&#8226;&nbsp;
                            <span class="groupuplink creatorlink"><%=creator_name%></span> <!-- <span class="groupuplink showmore m-corner-all"><i class="icon-eye"></i> Quick View</span> --> <span class="groupuplink showless m-corner-all" style="display:none"><i class="icon-eye-closed"></i> Hide Quick View</span><%if(this.commentcount){%>&nbsp;&#8226;&nbsp;<span class="<%=gid%>_<%=_id%>_commentcount"><%=commentcount%></span> Comments<%}%>
                        </div>
                        <%if(this.calendars){%>
                        <div style="padding-top:6px;">
                        <%for(var i = 0; i < calendars.length; i++){
                            var tcal=calendars[i];
                            %>
                            <%if(tcal.icon){%>
                            <span style="padding:1px 4px;background:<%=color%>;border:1px solid #ccc;font-size:12px;color:white;margin-left:2px" class="m-corner-all">
                                <i class="<%=tcal.icon%>"></i> <%=tcal.name%>
                            </span>
                            <%}else{%>
                            <span style="padding:1px 4px;background:<%=tcal.color%>;border:1px solid #ccc;font-size:12px;color:white;margin-left:2px" class="m-corner-all">
                                <i class="icon-calendar"></i> <%=tcal.name%>
                            </span>
                            <%}%>
                        <%}%>
                        </div>
                        <%}%>
                    </div>
                </td>
            </tr>
        </table>
    </div>
    <div class="extrainfo" style="display:none">
        <div style="font-size:12px;padding:5px;background:white;border:1px solid #ccc;margin:2px" class="m-corner-all commentbox redactorbox">
            <div style="font-size:16px;padding:5px 0px;"><b>Time:</b> <%=timestring%></div>
            <%if(this.location){%><div style="font-size:16px;padding:5px 0px;"><b>Location:</b> <%=location%><!--  <span class="groupuplink">(view map)</span> --></div><%}%>
            <%if(this.cost){%><div style="font-size:16px;padding:5px 0px;"><b>Cost:</b> $<%=cost%></div><%}%>
            <%if(this.capacity&&false){%>
            <div style="font-size:16px;padding:5px 0px;"><b>Capacity:</b> <%=capacity%> <%if(capacity=='∞'){%>(∞ remaining)<%}else{%>(<%=capacity-rsvp_yes%> remaining)<%}%></div>
            <%}%>
            <%if(this.description){%><div style="font-size:12px;padding:5px;border:1px solid #ccc;margin:5px 10px" class="m-corner-all commentbox redactorbox"><%=description%></div><%}%>
        </div>
    </div>
</div>
