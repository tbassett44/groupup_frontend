                                <div style="padding:10px;" class="menuicon viewpic <%if(view=='boxes'){%>activenav2<%}%>" data-type="boxes">
                                    <table style="width:100%;text-align:left;font-size:18px"><tr><td style="width:30px;text-align:center"><i class="icon-boxes" style="font-size:24px;"></i></td><td>Box View</td></tr></table>
                                </div>
                                <div style="height:1px;background:#555;"></div>
                                <div style="padding:10px;" class="menuicon viewpic <%if(view=='list'){%>activenav2<%}%>" data-type="list">
                                    <table style="width:100%;text-align:left;font-size:18px"><tr><td style="width:30px;text-align:center"><i class="icon-list" style="font-size:24px;"></i></td><td>List View</td></tr></table>
                                </div>
                            </div>
                        </span>
                    </td>
