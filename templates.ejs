@@@config_news@@@
<div data-<%=template%>="<%=_tid%>">
    <div style="padding:10px;text-align:left">
        <div class="modalheader"><i class="icon-gear"></i> News Feed Configuration</div>
        <!-- <div class="modalsubheader">Select the types of news types you are interested in</div> -->
        <div style="margin-top:15px;height:400px;position:relative;">
            <div style="position:absolute;top:0px;left:0;width:150px;bottom:0px;border:1px solid #ccc" class="m-corner-all truebox" id="categorycontainer">
                <div style="position:absolute;top:0;left:0;right:0;height:20px;;padding-top:3px;padding-left:5px;z-index:1" class="m-corner-top truebox themecolor">Categories <span style="font-size:10px"><i>(Drag to re-order)</i></span></div>
                <div style="position:absolute;top:20px;left:0;right:0;bottom:20px;z-index:2;overflow-y:scroll" data-id="newsconfigscroller">
                    <div id="categorysorter">
                    <%if(current.order.length){%>
                    <%for (var i = 0; i < current.order.length; i++) {
                        var feed=data[current.order[i]];
                        var info=(this.list&&list[current.order[i]])?list[current.order[i]]:[];
                        %>
                        <div class="newsconfselect truebox" data-type="<%=current.order[i]%>">
                            <table style="width:100%;">
                                <tr>
                                    <td style="width:50px;text-align:center">
                                        <img src="<%=feed.img%>" class="s-corner-all" style="max-width:40px;max-height:40px;">
                                    </td>
                                    <td>
                                        <div style="font-size:14px;padding-bottom:5px">
                                            <%=feed.name%>
                                        </div>
                                        <div style="font-size:10px;color:#888;padding-left:5px"><span id="<%=current.order[i]%>_feedcount"><%=info.length%></span> Feeds</div>
                                    </td>
                                </tr>
                            </table>
                        </div>
                    <%}%>
                    <%}else{%>
                        <div style="text-align:center;">
                            <div style="padding:20px;">No Categories Added</div>
                            <div style="padding-top:20px;">
                                <span class="btn btn-primary addcategory" style="font-size:13px;padding:2px"><i class="icon-gear"></i> Edit Categories</span>
                            </div>
                        </div>
                    <%}%>
                    </div>
                </div>
                <div style="position:absolute;bottom:0;left:0;right:0;height:20px;padding-top:3px;text-align:center;z-index:1" class="m-corner-bottom truebox themecolor">
                    <span class="menuicon addcategory"><i class="icon-gear"></i> Edit Categories</span>
                </div>
            </div>
            <div style="position:absolute;top:0;left:155px;right:0px;bottom:0;border:1px solid #ccc;" class="m-corner-all" id="feedselector">
            </div>
        </div>
    </div>
</div>
@@@categorypicker@@@
<div data-<%=template%>="<%=_tid%>">
    <div style="padding:10px;text-align:left">
        <div class="modalheader"><i class="icon-gear"></i> Category Picker</div>
    </div>
    <div style="margin:10px;border:1px solid #ccc;text-align:center;height:350px" class="m-corner-all" data-id="catscroller">
        <div>
            <%for (var i = 0; i < order.length; i++) {
                var item=list[order[i]];
                item.selected=(selected.indexOf(order[i])>=0)?1:0
                %>
                <div style="margin:5px;padding:5px;width:70px;height:90px;text-align:center;float:left;position:relative" class="truebox s-corner-all selectcategoryitem <%if(item.selected){%>selected<%}%>" data-id="<%=order[i]%>">
                    <div style="height:60px;">
                        <table style="width:100%;height:100%">
                            <tr>
                                <td>
                                    <img src="<%=item.img%>" style="max-width:60px;max-height:60px" class="s-corner-all">
                                </td>
                            </tr>
                        </table>
                    </div>
                    <div>
                        <div><%=item.name%></div>
                    </div>
                    <div style="position:absolute;top:0;left:0;padding:2px;text-align:center" class="themecolor selectedcontent s-corner-all">
                        <i class="icon-ok" style="font-size:16px"></i> 
                    </div>
                    <div style="position:absolute;top:0;left:0;padding:2px;text-align:center;background:#888;color:white" class="unselectedcontent s-corner-all">
                        <i class="icon-ok" style="font-size:16px"></i> 
                    </div>
                </div>
            <%};%>
            <div class="clearfloat"></div>
        </div>
    </div>
</div>
@@@feedselector@@@
<div data-<%=template%>="<%=_tid%>" style="width:100%;height:100%">
    <div style="position:absolute;top:0;left:0;right:0;height:20px;padding-left:5px;padding-top:3px;z-index:1" class="truebox m-corner-top themecolor">Select Feeds <span style="font-size:10px"><i>(Click to select)</i></span></div>
    <div data-id="selectscroller" style="position:absolute;top:20px;bottom:120px;left:0;right:0;z-index:2">
        <div>
        <%for (var i = 0; i < conf.feeds.order.length; i++) {
            var feed=conf.feeds.list[conf.feeds.order[i]];
            feed.source_info=(sources[feed.source])?sources[feed.source]:{name:'Undefined',image:''};
            feed.selected=(current.indexOf(conf.feeds.order[i])>=0)?1:0;
            if(feed.description) feed.source_info.description=feed.description;
            %>
            <div style="padding:5px;" data-id="<%=conf.feeds.order[i]%>" class="feedselector <%if(feed.selected){%>selected<%}%>">
                <table style="width:100%">
                    <tr>
                        <td style="width:70px;text-align:center;vertical-align:top">
                            <div>
                                <img src="<%=feed.source_info.image%>" style="max-width:60px;max-height:80px;">
                            </div>
                            <div>
                                <div class="feedselected" style="color:green"><i class="icon-ok"></i><i>Selected</i></div>
                                <div class="feednonselected" style="color:#888">Unselected</div>
                            </div>
                        </td>
                        <td style="vertical-align:top">
                            <div>
                                <div style="font-size:15px;font-weight:bold;padding-bottom:3px;"><%=feed.name%></div>
                                <div style="padding:5px;color:#555;font-size:12px">
                                    <%=feed.source_info.description%>
                                </div>
                                <div style="font-size:11px;padding-left:5px;padding-bottom:2px"><span style="padding:3px" class="groupuplink aboutsource" data-sourceid="<%=feed.source%>">More Info About <%=feed.source_info.name%></span></div>
                            </div>
                        </td>
                    </tr>
                </table>        
            </div>
        <%}%>
        </div>
    </div>
    <div style="position:absolute;bottom:0;left:0;right:0;height:120px">
        <div style="position:absolute;top:0;left:0;right:0;height:20px;padding-top:3px;padding-left:5px" class="truebox themecolor">Selected Feeds <span style="font-size:10px"><i>(Drag to re-order)</i></span></div>
        <div id="reorderfeeds" style="position:absolute;top:20px;left:0;right:0;bottom:0">

        </div>
    </div>
</div>
