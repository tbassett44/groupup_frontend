<div data-<%=template%>="<%=_tid%>" class="sfit">
    <div class="sfit" data-id="feedscroll">
        <div id="feedsortlist" style="height:100%;max-height:100%;">
        <%for (var i = 0; i < info.length; i++) {
            var data=info[i];%>
            <div class="selectedfeeditem m-corner-all" style="position:relative;float:left;width:70px;height:90px;text-align:center;margin:3px;border:1px solid #ccc;background:white;cursor:move" data-id="<%=data._id%>">
