    <div style="position:absolute;top:30px;left:0;right:0;bottom:0;z-index:1;" id="newsrenderarea" data-id="newsarea" class="viewpane">
        <div class="scrollcontainer">
            <div>
                <%if(conf.module=='government'){%>
                <div class="<%if(view=='list'){%>respcontent<%}else{%>scrolllist<%}%>"><div style="padding:5px;margin:5px;text-align:right;color:black" class="menuicon contactreps"><b><i class="icon-phone"></i> Contact Your Representatives</b></div></div>
                <%}%>
                <div class="pagescroll postcontent <%if(view=='list'){%>respcontent<%}else{%>scrolllist<%}%>"></div>
            </div>
