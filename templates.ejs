<div data-<%=template%>="<%=_tid%>" class="newsstory">
    <div>
        <table style="width:100%;">
            <tr>
                <td style="width:80px;text-align:left;vertical-align:top" class="notonsmall">
                    <%if(this.pic&&this.pic!='false'){%>
