@@@coinbase_funding@@@
<div data-<%=template%>="<%=_tid%>">
    <%if(sources&&sources.length){%>
    <div style="font-size:14px;padding-top:10px;padding-bottom:5px"><b>From Credit/Debit Card</b></div>
        <%for (var i = 0; i < sources.length; i++) {
            var source=sources[i];
            %>
            <div data-id="<%=source.pid%>" style="padding:4px;width:280px;cursor:pointer;" class="truebox fundselector s-corner-all <%if(sources.length==1||source.primary){%>selected<%}%>">
                <table>
                    <tr>
                        <td style="width:30px">
                            <i class="fundselected icon-check" style="font-size:20px;padding:4px;"></i>
                            <i class="fundnotselected icon-check-empty" style="font-size:20px;padding:4px;"></i>
                        </td>
                        <td style="padding-left:5px">
                            <div style="font-size:18px;" class="cbfunding"><i class="icon-credit-card webonly"></i> <%=source.name%></div>
                        </td>
                    </tr>
                </table>
            </div>
        <%};%>
        <div style="margin:10px">
            *Note - All Purchases with Debit/Credit cards will show up as "POS DEBIT COINBASE CHEAPSIDE"
        </div>
    <%}else{%>
    <div style="padding:20px;">
        <div style="font-size:14px;padding-bottom:6px">No Funding Sources Available, please go to Coinbase and add a credit or debit card.</div>
        <div style="padding-top:5px;"><span class="btn btn-primary x_updatecoinbase"><i class="icon-export"></i> Update On Coinbase</span></div>
    </div>
    <%}%>
</div>
