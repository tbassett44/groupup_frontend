@@@app@@@
<div class="sfit" data-<%=template%>="<%=_tid%>" style="z-index:5">
    <div class="sfit" id="appwrapper" data-<%=template%>="<%=_tid%>" style="z-index:2;">
        <div id="pageview" class="transition themecolor" style="display:none">
            <div id="mainpageheader" class="header truebox themecolor" style="font-size:20px;z-index:2">
                <table style="width:100%;height:100%;">
                    <tr>
                        <td id="pageheader"></td>
                        <td style="width:150px;text-align:center" id="profile"></td>
                    </tr>
                </table>
            </div>
            <div id="content">
                <div id="homepage" style="z-index:4;display:none;" class="sfit"></div>
                <div id="userbrain" style="z-index:4;display:none;" class="sfit"></div>
                <div id="gbrain" style="z-index:4;display:none;" class="sfit"></div>
                <div id="universe" style="z-index:4;display:none;" class="sfit"></div>
                <div id="loginscreen" style="display:none;" class="sfit">
                    <table style="width:100%;height:100%">
                        <tr><td>
                            <div style="width:300px;text-align:center;margin:auto;" class="themeFont">
                                <div style="padding:5px;" class="loginbox"></div>
                            </div>
                        </td></tr>
                    </table>
                </div>
                <div class="sfit searchcontainer" data-id="gsearchcontainer" style="z-index:5;display:none;top:0px;">
                    <div style="position:absolute;top:0;left:0;right:0;height:30px;background:#333;color:white;text-align:center;" class="truebox">
                        <table style="width:100%;height:100%;">
                            <tr>
                                <td class="searchresults"></td>
                            </tr>
                        </table>
                    </div>
                    <div style="position:absolute;top:0px;left:0;right:0;bottom:0;z-index:3;background:rgba(0,0,0,.6);
                    ">
                        <div id="mainsearch" class="searchlist m-corner-all" data-id="search">
                            <div class="m-corner-all truebox" style="background:#ddd;padding:20px" id="scrollsearch">
                                 <div id="search_users">
                                    <div style="font-size:25px;padding:3px;margin:3px;border-bottom:1px solid #aaa;color:#555;">Users</div>
                                    <div id="usersearchlist" class="searchresultboxes"></div>
                                    <div class="clearfloat"></div>
                                </div>
                                <div id="search_communities">
                                    <div style="font-size:25px;padding:3px;margin:3px;border-bottom:1px solid #aaa;color:#555;">Communities</div>
                                    <div id="communitysearchlist" class="searchresultboxes"></div>
                                    <div class="clearfloat"></div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="sfit" style="z-index:10;background:rgba(55,55,55,.4);display:none;" id="closemenubox"></div>
            </div>
        </div>
    </div>
</div>
@@@updatealert@@@
<div style="padding:5px 20px 10px 20px;text-align:left;font-size:14px;">
    <div class="modalheader" style="text-align:center">Required App Update</div>
    <div style="padding-bottom:10px;padding-top:10px">Seldomly, we make changes to the core of GroupUp to ensure that you recieve the best experience possible.</div>
    <div style="padding-bottom:10px">To keep to this commitment, we request that you update the app by clicking the button below.</div>
    <div style="padding-bottom:10px">Thanks!</div>
    <div style="padding-bottom:10px"><i class="icon-heart" style="color:red;font-size:20px;"></i></div>
</div>
@@@newsfeedwelcome@@@
<div data-<%=template%>="<%=_tid%>" style="text-align:left;">
    <div style="padding:10px;text-align:left">
        <div class="modalheader"><i class="icon-gear"></i> News Feed Configuration</div>
        <div class="modalsubheader">Please select the type of configuration you would like.  These settings can always be updated by clicking the <i class="icon-gear"></i> next to the News Feed.</div>
        <%if(!loggedin){%>
        <div style="padding:5px 5px 0px 5px;">
            <div class="s-corner-all" style="border:1px solid orange">
                <table>
                    <tr>
                        <td style="width:30px;text-align:center">
                            <i class="icon-warning-sign" style="color:orange;font-size:22px"></i>
                        </td>
                        <td style="padding:5px">You are not logged in and your settings may be lost.  Please log in or create an account to ensure your preferences are saved!</td>
                    </tr>
                </table>
            </div>
        </div>
        <%}%>
    </div>
    <div style="padding:20px">
        <div class="gbtn m-corner-all quickconf" style="width:100%;text-align:left;">
            <div style="font-size:16px;"><b>Quick Configuration</b></div> 
            <div style="padding-top:5px;">The quick configuration will set up some default categories and news feeds so you can get started easily. These can be customized later.</div>
        </div>
        <div class="gbtn m-corner-all customconf" style="width:100%;text-align:left;margin-top:15px">
            <div style="font-size:16px;"><b>Custom Configuration</b></div>
            <div style="padding-top:5px">The custom configuration will allow you to individaully select the categories and feeds you are interested in.</div>
        </div>
    </div>
</div>
