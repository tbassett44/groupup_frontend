                <td style="padding-top:10px;vertical-align:top;">
                    <select id="everyselect">
                        <% var c=1;
                        while(c<7){%>
                            <option value="<%=c%>" <%if(c==every){%>SELECTED<%}%>><%=c%></option>
                        <% c++;
                        }%>
                    </select> <span id="calendarinterval">weeks</span>
                </td>
            </tr>
            <tr id="repeatrow" <%if(repeat=='yearly'){%>style="display:none"<%}%>>
                <td style="width:120px;text-align:right;font-weight:bold;padding-right:5px;font-size:15px;padding-top:10px;vertical-align:top">Repeat On:</td>
                <td style="padding-top:10px;vertical-align:top;">
                    <div id="weeklyselector" <%if(repeat!='weekly'){%>style="display:none"<%}%>>
                        <table style="text-align:center" id="weeklycheckboxes">
                            <tr>
                                <td><label style="font-size:14px;padding:2px 8px;cursor:pointer" for="weekday_sun">S</label></td>
                                <td><label style="font-size:14px;padding:2px 8px;cursor:pointer" for="weekday_mon">M</label></td>
                                <td><label style="font-size:14px;padding:2px 8px;cursor:pointer" for="weekday_tue">T</label></td>
                                <td><label style="font-size:14px;padding:2px 8px;cursor:pointer" for="weekday_wed">W</label></td>
                                <td><label style="font-size:14px;padding:2px 8px;cursor:pointer" for="weekday_thu">T</label></td>
                                <td><label style="font-size:14px;padding:2px 8px;cursor:pointer" for="weekday_fri">F</label></td>
                                <td><label style="font-size:14px;padding:2px 8px;cursor:pointer" for="weekday_sat">S</label></td>
                            </tr>
                            <tr>
                                <td><input class="weekselect" type="checkbox" style="font-size:18px;cursor:pointer" id="weekday_sun" value="0" <%if(this.weekrepeat&&(weekrepeat.indexOf(0)>=0||weekrepeat.indexOf("0")>=0)){%>CHECKED<%}%>></td>
                                <td><input class="weekselect" type="checkbox" style="font-size:18px;cursor:pointer" id="weekday_mon" value="1" <%if(this.weekrepeat&&(weekrepeat.indexOf(1)>=0||weekrepeat.indexOf("1")>=0)){%>CHECKED<%}%>></td>
                                <td><input class="weekselect" type="checkbox" style="font-size:18px;cursor:pointer" id="weekday_tue" value="2" <%if(this.weekrepeat&&(weekrepeat.indexOf(2)>=0||weekrepeat.indexOf("2")>=0)){%>CHECKED<%}%>></td>
                                <td><input class="weekselect" type="checkbox" style="font-size:18px;cursor:pointer" id="weekday_wed" value="3" <%if(this.weekrepeat&&(weekrepeat.indexOf(3)>=0||weekrepeat.indexOf("3")>=0)){%>CHECKED<%}%>></td>
                                <td><input class="weekselect" type="checkbox" style="font-size:18px;cursor:pointer" id="weekday_thu" value="4" <%if(this.weekrepeat&&(weekrepeat.indexOf(4)>=0||weekrepeat.indexOf("4")>=0)){%>CHECKED<%}%>></td>
                                <td><input class="weekselect" type="checkbox" style="font-size:18px;cursor:pointer" id="weekday_fri" value="5" <%if(this.weekrepeat&&(weekrepeat.indexOf(5)>=0||weekrepeat.indexOf("5")>=0)){%>CHECKED<%}%>></td>
                                <td><input class="weekselect" type="checkbox" style="font-size:18px;cursor:pointer" id="weekday_sat" value="6" <%if(this.weekrepeat&&(weekrepeat.indexOf(6)>=0||weekrepeat.indexOf("6")>=0)){%>CHECKED<%}%>></td>
                            </tr>
                        </table>
                    </div>
                    <div id="monthlyselector" <%if(repeat!='monthly'){%>style="display:none"<%}%>>
                        <input type="radio" class="monthlyrepeat" name="monthlyrepeat" id="monthday" value="day" <%if(monthlyrepeat=='day'){%>CHECKED<%}%>><label for="monthday">Day of the Month</label>
                        <input type="radio" class="monthlyrepeat" name="monthlyrepeat" id="monthdayofweek" value="dow" style="margin-left:5px" <%if(monthlyrepeat=='dow'){%>CHECKED<%}%>><label for="monthdayofweek">Day of the Week</label>
                    </div>
                </td>
            </tr>
            <tr>
                <td style="width:120px;text-align:right;font-weight:bold;padding-right:5px;font-size:15px;padding-top:10px;vertical-align:top">Ends:</td>
                <td style="padding-top:10px;vertical-align:top;">
                    <table>
                        <tr>
                            <td style="padding-top:5px"><input class="endradio" type="radio" name="ends" value="never" id="neverradio" <%if(ends=='never'){%>CHECKED<%}%>></td>
                            <td style="padding-top:5px"><label for="neverradio">Never</label></td>
                        </tr>
                        <tr>
                            <td style="padding-top:5px"><input class="endradio" type="radio" name="ends" value="after" id="afterradio" <%if(ends=='after'){%>CHECKED<%}%>></td>
                            <td style="padding-top:5px"><label for="afterradio">After:</label> <input id="afternumber" type="number" style="width:30px;padding:2px 4px;border:1px solid #ccc;" class="s-corner-all" <%if(ends!='after'){%>disabled<%}%> value="<%=endafter%>"> occurances</td>
                        </tr>
                        <tr>
                            <td style="padding-top:5px"><input class="endradio" type="radio" name="ends" value="on" id="onradio" <%if(ends=='on'){%>CHECKED<%}%>></td>
                            <td style="padding-top:5px"><label for="onradio">On:</label> <div style="position:relative;display:inline-block;"><input id="afterdate" type="text" style="font-size:12px;padding:2px 5px;width:100px;border:1px solid #ccc;" class="s-corner-all" <%if(ends!='on'){%>disabled<%}%> value="<%=endafterdate%>"><div class="sfit" id="aftercover" style="z-index:1;cursor:pointer;"></div></td>
                        </tr>
                    </table>
                </td>
            </tr>
        </table>
    </div>
    <div style="text-align:center;max-width:300px;margin:auto;padding-top:15px">
        <div style="font-size:15px;text-align:left;padding-bottom:5px;"><b>Summary</b></div>
        <div style="font-size:18px;padding:5px;border:1px solid #ccc;background:#eee;" class="s-corner-all" id="repeatsummary"></div>
    </div>
</div>
